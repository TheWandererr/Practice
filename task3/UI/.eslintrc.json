{
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "extends": "eslint-config-google",
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es6": true
    },
    "rules": {
        "require-jsdoc": "off",
        "linebreak-style": ["error", "windows"],
        "indent": "off",
        "no-console": "off",
        "quotes": ["error", "single"],
        "max-len": ["error", 130],
        "no-unused-vars": ["warn", {"vars":  "local", "args": "after-used", "ignoreRestSiblings":  false}]
    }
}